[alias]
	ad = add --update
	br = branch
	co = checkout
	dc = diff --ignore-all-space --find-renames --cached
	dcsv = diff --color-words="[^[:space:],]+"
	df = diff
	dh = diff --ignore-all-space --find-renames HEAD
	dw = diff --find-renames --color-words=.
	gr = log -13 --all --oneline --graph
	lf = log --format=fuller
	rba = rebase --abort
	rbc = rebase --continue
	rbi = rebase --interactive
	re = remote
	st = status
	alias = ! git config --get-regexp ^alias\\. | sed -e s/^alias\\.// -e s/\\ /\\ =\\ /
[mergetool]
	keepBackup = false
[rerere]
	enabled = true
