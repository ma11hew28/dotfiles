[alias]
	ad = add --update
	alias = ! git config --get-regexp ^alias\\. | sed -e s/^alias\\.// -e s/\\ /\\ =\\ /
	br = branch
	co = checkout
	dc = diff --cached
	dcsv = diff --color-words="[^[:space:],]+"
	df = diff
	dh = diff HEAD
	dw = diff --color-words=.
	gr = log -13 --all --oneline --graph
	lf = log --format=fuller
	rba = rebase --abort
	rbc = rebase --continue
	rbi = rebase --interactive
	re = remote
	st = status
[mergetool]
	keepBackup = false
[rerere]
	enabled = true
