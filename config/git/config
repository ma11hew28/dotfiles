[alias]
	ad = add --update
	alias = !git help -a | sed '1,/aliases$/d'
	br = branch
	co = checkout
	cr = clone --recurse-submodules
	dc = diff --cached
	dcm = diff -U0 --color-moved
	dcsv = diff -U0 --color-words="[^[:space:],]+"
	dd = diff -U0 --color-words=.
	df = diff
	dh = diff HEAD
	du = diff -U0
	dw = diff -U0 --color-words
	dwc = diff -U0 --color-words --cached
	gr = log -13 --all --oneline --graph
	lf = log --format=fuller
	rba = rebase --abort
	rbc = rebase --continue
	rbi = rebase --interactive
	re = remote
	st = status
[mergetool]
	keepBackup = false
[rerere]
	enabled = true
